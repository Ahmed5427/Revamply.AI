{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "N8N_WEBHOOK_URL": "https://afxv17.app.n8n.cloud/webhook/revamply-form",
    "N8N_BLUEPRINT_WEBHOOK_URL": "https://revamply-ai.vercel.app/api/receive-blueprint"
  }
}
